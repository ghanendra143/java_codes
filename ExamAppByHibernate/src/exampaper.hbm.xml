<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.model.ExamPaper" table = "ExamPaperTable">
    
      <id name = "quesId" type = "int" column = "quesId">
         <generator class="native"/>
      </id>
      
      <property name = "question" />
      <property name = "opt1" />
      <property name = "opt2" />
      <property name = "opt3" />
      <property name = "opt4" />
      <property name = "realAnswer" />
      
   </class>
</hibernate-mapping>