<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.model.Student" table = "StudentsTable">
       
      <id name = "studId" type = "int" column = "studId">
         <generator class="assigned"/>
      </id>
      
      <property name = "studFirstName" />
      <property name = "studLastName" />
      <property name = "marks" />
      
   </class>
</hibernate-mapping>